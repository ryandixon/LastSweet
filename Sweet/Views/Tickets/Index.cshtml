@model IEnumerable<Sweet.Models.Ticket>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Tickets</h2>
<p>
    @Html.ActionLink("Add Ticket", "New")
</p>
<ul>
    @foreach (var ticket in Model)
    {
        <li>
            @ticket.Title (@ticket.Resolve)
            @Html.ActionLink("Edit", "Edit", new { id = ticket.TicketId }) |
            @Html.ActionLink("Delete", "Delete", new { id = ticket.TicketId })
        </li>
    }
</ul>

